  523  grep -r lan ./*
  524  grep -r local ./*
  525  nano /usr/local/bin/cleanup_captures.sh
  526  crontab -e
  527  chmod +x /usr/local/bin/cleanup_captures.sh
  528  ls -lah /usr/local/bin/
  529  journalctl -b -t ip-monitor
